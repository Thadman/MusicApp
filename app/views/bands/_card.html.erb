<div class="card text-center my-3 col-md-4 col-xs-12" style="max-width:30%; min-width:18rem;">
  <%= image_tag image_for(band) , alt: "#{band.name} image", class: 'card-img-top img_fluid' %>
  <div class="card-body">
    <h5 class="card-title"><%= band.name %></h5>
    <h6 class="card-subtitle mb-2 text-muted"><%= band.album_title %></h6>
    <%= link_to 'View', band_path(band), class: 'btn btn-primary' %>
  </div>
</div>

